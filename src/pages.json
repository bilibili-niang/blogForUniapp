{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^ice-(.*)": "@/components/ice$1.vue",
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue"
    }
  },
  "pages": [
   /* {
      "path": "pages/mainContainer/index",
      "style": {
        "navigationBarTitleText": "容器",
        "enablePullDownRefresh": true
      }
    },*/
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/about/index",
      "style": {
        "navigationBarTitleText": "关于此tag",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/read/index",
      "style": {
        "navigationBarTitleText": "阅读",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/addEvents/index",
      "style": {
        "navigationBarTitleText": "今日"
      }
    },
    {
      "path": "pages/list/index",
      "style": {
        "navigationBarTitleText": "列表",
        "enablePullDownRefresh": true,
        "onReachBottomDistance": 200
      }
    },
    {
      "path": "pages/eventsList/index",
      "style": {
        "navigationBarTitleText": "昨天",
        "enablePullDownRefresh": true,
        "onReachBottomDistance": 200
      }
    },
    {
      "path": "pages/mine/index",
      "style": {
        "navigationBarTitleText": "mine",
        "enablePullDownRefresh": true,
        "onReachBottomDistance": 200
      }
    },
    {
      "path": "pages/bindWebAccount/index",
      "style": {
        "navigationBarTitleText": "绑定我的pc端账户",
        "enablePullDownRefresh": true,
        "onReachBottomDistance": 200
      }
    },
    {
      "path": "pages/mine/changeUserInfo/index",
      "style": {
        "navigationBarTitleText": "修改信息",
        "enablePullDownRefresh": true,
        "onReachBottomDistance": 200
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#202124",
    "backgroundTextStyle": "dark",
    "color": "#ffffff",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    // "backgroundColor": "#dcdadd",
    "color": "#333333",
    // "selectedColor": "#441a1e",
    "selectedColor": "#541e24",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/icon/index.png",
        "selectedIconPath": "static/icon/indexActive.png"
      },
      {
        "pagePath": "pages/list/index",
        "text": "列表",
        "iconPath": "static/icon/list.png",
        "selectedIconPath": "static/icon/listActive.png"
      },
      {
        "pagePath": "pages/addEvents/index",
        "text": "今日",
        "iconPath": "static/icon/today.png",
        "selectedIconPath": "static/icon/todayActive.png"
      },
      /*{
        "pagePath": "pages/eventsList/index",
        "text": "昨天",
        "iconPath": "static/icon/eventsList.png",
        "selectedIconPath": "static/icon/eventsListActive.png"
      },*/
      {
        "pagePath": "pages/mine/index",
        "text": "我的",
        "iconPath": "static/icon/mine.png",
        "selectedIconPath": "static/icon/mineActive.png"
      }
    ]
  }
}
